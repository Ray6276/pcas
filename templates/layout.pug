- function convertDate(date) { var year = date.getUTCFullYear(); var month = date.getUTCMonth() + 1; var day = date.getUTCDate(); if (month < 10) { month = '0' + month}; if (day < 10) { day = '0' + day}; return year + '-' + month + '-' + day; }

